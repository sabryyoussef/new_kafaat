<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

        <!-- Intake Batch Sequence -->
        <record id="sequence_intake_batch" model="ir.sequence">
            <field name="name">Grants Training Intake Batch</field>
            <field name="code">gr.intake.batch</field>
            <field name="prefix">BATCH</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Assignment Sequence -->
        <record id="sequence_assignment" model="ir.sequence">
            <field name="name">Grants Training Assignment</field>
            <field name="code">gr.assignment</field>
            <field name="prefix">ASG</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Document Request Sequence -->
        <record id="sequence_document_request" model="ir.sequence">
            <field name="name">Grants Training Document Request</field>
            <field name="code">gr.document.request</field>
            <field name="prefix">DOC</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Homework Attempt Sequence -->
        <record id="sequence_homework_attempt" model="ir.sequence">
            <field name="name">Grants Training Homework Attempt</field>
            <field name="code">gr.homework.attempt</field>
            <field name="prefix">HW</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Certificate Sequence -->
        <record id="sequence_certificate" model="ir.sequence">
            <field name="name">Grants Training Certificate</field>
            <field name="code">gr.certificate</field>
            <field name="prefix">CERT</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Document Request Portal Sequence -->
        <record id="sequence_document_request_portal" model="ir.sequence">
            <field name="name">Student Document Request</field>
            <field name="code">gr.document.request.portal</field>
            <field name="prefix">DOCREQ</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Course Enrollment Request Sequence -->
        <record id="sequence_course_enrollment_request" model="ir.sequence">
            <field name="name">Course Enrollment Request</field>
            <field name="code">course.enrollment.request</field>
            <field name="prefix">ENR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

</odoo>
